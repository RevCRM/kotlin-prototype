{
  "name": "revcrm",
  "version": "0.0.2",
  "description": "RevCRM - Open-source CRM solution built on RevJS",
  "main": "./dist/server.js",
  "repository": "https://github.com/RevCRM/revcrm.git",
  "author": "Russell Briggs <revcrm@paraflyer.net>",
  "license": "MIT",
  "scripts": {
    "build": "webpack --config ./conf/webpack.config.js",
    "build-watch": "webpack --config ./conf/webpack.config.js --watch",
    "build-libs": "webpack --config ./conf/webpack.clientlibs.config.js",
    "start": "node ./dist/server/server.js",
    "start-watch": "nodemon ./dist/server/server.js",
    "client-webpack-stats": "webpack --config ./conf/webpack.client.config.js --profile --json > client-webpack-stats.json",
    "sync-deps": "node ./scripts/sync-deps.js"
  },
  "dependencies": {
    "flexboxgrid": "6.3.1",
    "hapi": "16.1.0",
    "inert": "4.1.0",
    "material-ui": "0.17.0",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-redux": "5.0.3",
    "react-router": "4.0.0",
    "react-tap-event-plugin": "2.0.1",
    "redux": "3.6.0",
    "redux-form": "6.5.0",
    "rev-api": "0.0.1",
    "rev-api-hapi": "0.0.1",
    "rev-forms": "0.0.1",
    "rev-forms-redux-mui": "0.0.1",
    "rev-models": "0.0.3",
    "source-map-support": "0.4.11",
    "yar": "8.1.2"
  },
  "devDependencies": {
    "@types/es6-promise": "0.0.32",
    "@types/hapi": "16.0.0",
    "@types/inert": "4.0.0",
    "@types/material-ui": "0.16.57",
    "@types/node": "7.0.8",
    "@types/react": "15.0.16",
    "@types/react-dom": "0.14.23",
    "@types/react-redux": "4.4.37",
    "@types/react-router": "3.0.7",
    "@types/react-tap-event-plugin": "0.0.30",
    "@types/redux-form": "6.3.6",
    "awesome-typescript-loader": "3.1.2",
    "copy-webpack-plugin": "4.0.1",
    "nodemon": "1.11.0",
    "tslint": "4.5.1",
    "tslint-loader": "3.4.3",
    "typescript": "2.2.1",
    "webpack": "2.2.1",
    "webpack-node-externals": "1.5.4"
  }
}
