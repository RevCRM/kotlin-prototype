{
  "name": "revcrm",
  "version": "0.0.2",
  "description": "RevCRM - Open-source CRM solution built on RevJS",
  "main": "./dist/server.js",
  "repository": "https://github.com/RevCRM/revcrm.git",
  "author": "Russell Briggs <revcrm@paraflyer.net>",
  "license": "MIT",
  "scripts": {
    "build": "webpack --config ./conf/webpack.config.js",
    "build-watch": "webpack --config ./conf/webpack.config.js --watch",
    "build-libs": "webpack --config ./conf/webpack.clientlibs.config.js",
    "start": "node ./dist/server/server.js",
    "start-watch": "nodemon ./dist/server/server.js",
    "client-webpack-stats": "webpack --config ./conf/webpack.client.config.js --profile --json > client-webpack-stats.json",
    "sync-deps": "node ./scripts/sync-deps.js"
  },
  "dependencies": {
    "flexboxgrid": "6.3.1",
    "hapi": "16.3.0",
    "inert": "4.2.0",
    "material-ui": "0.18.1",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-redux": "5.0.5",
    "react-router-dom": "4.1.1",
    "react-tap-event-plugin": "2.0.1",
    "redux": "3.6.0",
    "redux-form": "6.7.0",
    "redux-thunk": "2.2.0",
    "rev-api": "0.0.1",
    "rev-api-hapi": "0.0.1",
    "rev-forms": "0.0.1",
    "rev-forms-redux-mui": "0.0.1",
    "rev-models": "0.0.1",
    "source-map-support": "0.4.15",
    "yar": "8.1.2"
  },
  "devDependencies": {
    "@types/hapi": "16.1.2",
    "@types/inert": "4.2.1",
    "@types/material-ui": "0.17.10",
    "@types/node": "7.0.27",
    "@types/react": "15.0.26",
    "@types/react-dom": "15.5.0",
    "@types/react-redux": "4.4.41",
    "@types/react-router-dom": "4.0.4",
    "@types/react-tap-event-plugin": "0.0.30",
    "@types/redux-form": "6.6.2",
    "awesome-typescript-loader": "3.1.3",
    "copy-webpack-plugin": "4.0.1",
    "fs-extra": "3.0.1",
    "nodemon": "1.11.0",
    "tslint": "5.4.2",
    "tslint-loader": "3.5.3",
    "typescript": "2.3.4",
    "webpack": "2.6.1",
    "webpack-node-externals": "1.6.0"
  }
}
